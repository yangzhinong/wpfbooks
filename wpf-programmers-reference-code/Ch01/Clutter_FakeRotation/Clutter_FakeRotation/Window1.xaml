						<GradientStop Color="#FFA0FFA0" Offset="0"/>
						<GradientStop Color="#FF008000" Offset="1"/>
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Height" Value="40"/>
			<Setter Property="Foreground" Value="Red"/>
			<Setter Property="BitmapEffect">
				<Setter.Value>
					<BevelBitmapEffect/>
				</Setter.Value>
