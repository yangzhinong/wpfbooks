<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="StrokeDashCaps.Window1"
	x:Name="Window"
	Title="StrokeDashCaps"
	Width="400" Height="220" Background="Yellow"
	FontSize="25" FontWeight="Bold">
	<StackPanel>
		<StackPanel Orientation="Horizontal">
			<Label Width="120" Content="Flat"/>
		    <Line Margin="10" Height="20" X1="0" Y1="10" X2="235" Y2="10" Stroke="Green"
		     StrokeThickness="15" StrokeDashArray="3" StrokeDashCap="Flat"/>
		</StackPanel>
		<StackPanel Orientation="Horizontal">
			<Label Width="120" Content="Square"/>
		    <Line Margin="10" Height="20" X1="0" Y1="10" X2="235" Y2="10" Stroke="Green"
		     StrokeThickness="15" StrokeDashArray="3" StrokeDashCap="Square"/>
		</StackPanel>
		<StackPanel Orientation="Horizontal">
			<Label Width="120" Content="Round"/>
		    <Line Margin="10" Height="20" X1="0" Y1="10" X2="235" Y2="10" Stroke="Green"
		     StrokeThickness="15" StrokeDashArray="3" StrokeDashCap="Round"/>
		</StackPanel>
		<StackPanel Orientation="Horizontal">
			<Label Width="120" Content="Triangle"/>
		    <Line Margin="10" Height="20" X1="0" Y1="10" X2="235" Y2="10" Stroke="Green"
		     StrokeThickness="15" StrokeDashArray="3" StrokeDashCap="Triangle"/>
		</StackPanel>
	</StackPanel>
</Window>