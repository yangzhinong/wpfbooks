<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Storyboard x:Key="DockAnimation">
		<DoubleAnimation Storyboard.TargetProperty="(Viewport3D.Children)[0].(ContainerUIElement3D.Children)[0].(ModelUIElement3D.Model).(GeometryModel3D.Transform).(Transform3DGroup.Children)[0].(RotateTransform3D.Rotation).(AxisAngleRotation3D.Angle)"
						 Duration="0:0:0.5"
						 From="0"
						 To="90" />
	</Storyboard>

	<Storyboard x:Key="SlideAnimation">
		<DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TranslateTransform.X)"
						 Duration="0:0:0.5"
						 To="0" />
		<DoubleAnimation Storyboard.TargetProperty="Opacity"
						 Duration="0:0:0.5" />
	</Storyboard>

</ResourceDictionary>